<div class="row">
  <div class="large-3 columns">
    <%= link_to "Back", profile_path(), class: "button" %>
  </div>
  <div class="large-4 columns end">
    <ul class="link-list friendsList section">
      <% @user_friendships.each do |friend| %>
        <li class="portfolio-preview"><%= avatar_profile_link friend, {}, class: '' %></li>
      <% end %>
    </ul>
  </div>

  <div class="large-9 columns large-3-offset">
    <%= will_paginate @user_friendships %>
  </div>
</div>
